<div class="change-view">
  <div class="sub-footer">
  <% if user_signed_in? %>
    <%= link_to "マイページ", "#", class: "btn btn-outline-light sub-btn" %>
    <%= link_to "ログアウト", destroy_user_session_path, method: :delete, class: "btn btn-outline-light sub-btn" %>
  <% elsif admin_signed_in? %>
    <%= link_to "ADMIN PAGE", admin_path(current_admin), class: "btn btn-outline-light sub-btn" %>
  <% else %>
    <%= link_to "ログイン", user_session_path, class: "btn btn-outline-light sub-btn" %>
    <%= link_to "新規登録", new_user_registration_path, class: "btn btn-outline-light sub-btn" %>
  <% end %>
  </div>
</div>