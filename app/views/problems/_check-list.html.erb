    <div class="grade-problems">
      <div class="problem5">6級</div>
      <% @problems2.each do |problem| %>
        <% unless problem.results.where(user_id: current_user.id).present? %>
          <%= link_to problem.name, gym_problem_list_problem_results_path(@gym, @problem_list ,problem), class: "btn btn-outline-dark btn-size12" %>
        <% else %>
          <%= link_to problem.name, edit_gym_problem_list_problem_result_path(@gym, @problem_list ,problem,problem.results.where(user_id: current_user.id)), class: "btn btn-danger btn-size13" %>
